/* <let> avec 3 declarations de variable et fonction, et 3 instructions et appels de fonction */

let
	var i := 0
	var j := 0
	var k := 0

	function test1() = print("test")

	function test2() = test1()

	function main() = test2()
in
	(printi(i);
	printi(j);
	printi(k);
	test1();
	test2();
	main())
end
