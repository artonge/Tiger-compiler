###
/* 1 commentaire, sans instruction avant, sur 1 ligne */

/* commentaire */

let
	function main() = print("test")
in main() end
###
/* 1 commentaire, sans instruction avant, sur plusieurs lignes */

/*
1
2
3
*/

let
	function main() = print("test")
in main() end
###
/* 1 commentaire, apres instruction, sur 1 ligne */

let
	function main() = print("test")
	/* commentaire */
in main() end
###
/* 1 commentaire, apres instruction, sur plusieurs lignes */

let
	function main() = print("test")
	/*
	1
	2
	3
	*/
in main() end
