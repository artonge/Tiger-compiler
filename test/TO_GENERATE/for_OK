###
/* <for> simple croissant */

let
	function main() =
		for i := 0 to 10 do
			print("test")
in main() end
###
/* <for> simple decroissant */

let
	function main() =
		for i := 10 to 0 do
			print("test")
in main() end
###
/* <for> sans instruction */

let
	function main() =
		for i := 0 to 3 do
in main() end
###
/* <for> avec ligne vide */

let
	function main() =
		for i := 0 to 3 do

in main() end
###
/* double-imbrication de <for> */

let
	function main()() =
		for i := 0 to 3 do
			for j := 4 to 7 do
				printi(i+j)
in main() end
###
/* triple-imbrication de <for> */

let
	function main()() =
		for i := 0 to 3 do
			for j := 4 to 7 do
				for k := 8 to 10 do
					printi(i+j+k)
in main() end
###
/* <for> avec reutilisation de compteur */

let
	function main()() =
		for i := 0 to 3 do
			for j := i+4 to 10 do
				printi(i+j)
in main() end
###
/* <for> avec reutilisation de compteurs */

let
	function main()() =
		for i := 0 to 3 do
			for j := 4 to i+4 do
				for k := j-4 to 4 do
					printi(i+j+k)
in main() end